{
  "name": "travis-token-updater",
  "description": "tool for rolling tokens for multiple projects across an account",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/travis-token-updater",
  "bugs": "https://github.com/travi/travis-token-updater/issues",
  "homepage": "https://npm.im/travis-token-updater",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "test": "npm-run-all --print-label --parallel lint:* test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "^1.8.5",
    "@travi/eslint-config-travi": "^1.8.0",
    "babel-preset-travi": "^1.4.2",
    "babel-register": "^6.26.0",
    "ban-sensitive-files": "^1.9.2",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "commitlint-config-travi": "^1.2.38",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "sinon": "^7.2.3",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^16.15.0",
    "inquirer": "^6.2.2",
    "js-yaml": "^3.12.1",
    "listr": "^0.14.3",
    "listr-inquirer": "^0.1.0",
    "netrc": "^0.1.4"
  }
}
