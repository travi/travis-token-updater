{
  "name": "travis-token-updater",
  "description": "tool for rolling tokens for multiple projects across an account",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/travis-token-updater",
  "bugs": "https://github.com/travi/travis-token-updater/issues",
  "homepage": "https://npm.im/travis-token-updater",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:gherkin": "gherkin-lint",
    "lint:sensitive": "ban",
    "pretest": "run-s clean",
    "test": "npm-run-all --print-label build --parallel lint:* test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "pregenerate:md": "run-s build:js",
    "generate:md": "remark . --output",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.18.9",
    "@cucumber/cucumber": "8.4.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@travi/any": "2.1.5",
    "@travi/babel-preset": "3.0.89",
    "@travi/eslint-config": "3.0.5",
    "@travi/eslint-config-cucumber": "1.0.6",
    "@travi/eslint-config-mocha": "1.0.18",
    "ban-sensitive-files": "1.9.18",
    "c8": "7.11.3",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "commitlint-config-travi": "1.4.16",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "debug": "4.3.4",
    "gherkin-lint": "4.2.2",
    "http-status-codes": "2.2.0",
    "husky": "8.0.1",
    "lodash": "4.17.21",
    "mocha": "7.2.0",
    "nock": "13.2.8",
    "npm-run-all": "4.1.5",
    "remark": "14.0.2",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "4.0.1",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.77.0",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "14.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^16.33.1",
    "execa": "^4.0.0",
    "inquirer": "^8.0.0",
    "js-yaml": "^4.0.0",
    "listr": "^0.14.3",
    "netrc": "^0.1.4",
    "travis-ci": "2.2.0"
  }
}
