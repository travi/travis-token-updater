{
  "name": "travis-token-updater",
  "description": "tool for rolling tokens for multiple projects across an account",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/travis-token-updater",
  "bugs": "https://github.com/travi/travis-token-updater/issues",
  "homepage": "https://npm.im/travis-token-updater",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:gherkin": "gherkin-lint",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "test": "npm-run-all --print-label --parallel lint:* test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "^7.5.5",
    "@travi/any": "^2.0.3",
    "@travi/babel-preset": "3.0.0",
    "@travi/eslint-config": "1.0.20",
    "@travi/eslint-config-cucumber": "1.0.3",
    "@travi/eslint-config-mocha": "1.0.3",
    "ban-sensitive-files": "^1.9.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.1.0",
    "commitlint-config-travi": "^1.3.3",
    "cucumber": "^5.1.0",
    "cz-conventional-changelog": "^3.0.2",
    "debug": "^4.1.1",
    "gherkin-lint": "^3.3.6",
    "http-status-codes": "^1.3.2",
    "husky": "^3.0.4",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.20.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sinon": "^7.4.1",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^16.28.7",
    "execa": "^2.0.4",
    "inquirer": "^7.0.0",
    "js-yaml": "^3.12.1",
    "listr": "^0.14.3",
    "netrc": "^0.1.4",
    "travis-ci": "2.2.0"
  }
}
